            <column name="OPERATE_TIME" length="200" />
        </property>
         <property name="removed" type="string">
            <column name="removed" length="200" />
        </property>
         <property name="flag" type="string">
            <column name="flag" length="20" />
        </property>
        <property
            name="personName"
            type="java.lang.String"
            update="true"
            insert="true"
            column="PERSONNAME"
            length="4000"
        />
        
        <property
            name="redattach"
            type="java.lang.String"
            update="true"
            insert="true"
            column="DEALFILE"
            length="4000"
        />
        
         <property
            name="chiefPerson"
            type="java.lang.String"
            update="true"
            insert="true"
            column="CHIEF_PERSON"
            length="200"
        />
        
         <property
            name="typeTitle"
            type="java.lang.String"
            update="true"
            insert="true"
            column="TYPE_TITLE"
            length="200"
        />
        
        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Directive.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>
